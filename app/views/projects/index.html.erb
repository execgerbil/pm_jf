<h1>All Projects</h1>

 <%= link_to "Create New Project", new_project_path %>

 <% @projects.each do |project| %>
    <h2><%= link_to project.title, project_path(project.id) %></h2>
      <p><%= project.description %></p>
      <p> Created On: <%= project.created_at.strftime("%Y-%b-%d") %>
      </p>
      <hr>
      <% end %>
